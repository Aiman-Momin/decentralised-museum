{
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "dist/public",
  "devCommand": "npm run dev",
  "functions": {
    "dist/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
